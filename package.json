{
  "name": "discord-runner",
  "version": "0.0.1",
  "description": "A Discord bot for the Agora Space platform",
  "author": "ZGEN",
  "license": "MIT",
  "homepage": "https://github.com/AgoraSpaceDAO/discord-runner#readme",
  "bugs": {
    "url": "https://github.com/AgoraSpaceDAO/discord-runner/issues"
  },
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev src/Main.ts",
